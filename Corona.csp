	.equ @, $000	
	.equ keyboard_status, $000
	.equ keyboard_data, $001



	LDX# $001

poll_keybd:	INB keyboard_status
	JGE poll_keybd
	INB keyboard_data
	STA+ $D00
	CMX# $009
	JGE verify
	ADX# $001
	JMP poll_keybd
	
verify:

erase:	.equ @, $000
	LDA# $FFF
	STA $000
	LDX# $001
	LDA# $000
	STA+ $001
	ADX# $001
	CMX $000
	JEQ end


end:	hlt